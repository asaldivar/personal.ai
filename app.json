{
  "expo": {
    "name": "Chat",
    "slug": "Chat",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "chat",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "updates": {
      "enabled": true
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.chat"
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "package": "com.yourcompany.chat"
    },
    "web": {
      "bundler": "metro",
      "output": "static"
    },
    "plugins": ["expo-router", "expo-splash-screen", "expo-updates"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
